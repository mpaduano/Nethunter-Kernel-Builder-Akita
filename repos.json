[
  {
    "name": "pixel8a-akita",
    "androidVersion": "15",
    "kernelVersion": "6.1",
    "buildSystem": "bazel",

    "kernelSources": {
      "common": {
        "repo": "https://android.googlesource.com/kernel/common",
        "tags": "android-15.0.0_r0.16"
      },
      "modules": {
        "repo": "https://github.com/LineageOS/android_device_google_akita-kernels",
        "branch": "lineage-23.0"
      },
      "device": {
        "repo": "https://android.googlesource.com/kernel/devices/google/akita",
        "branch": "android-gs-akita-6.1-android15-qpr2"
      },
      "manifest": {
        "repo": "git clone https://github.com/etnperlong/manifest-6.1-akita",
        "branch": "android-gs-akita-6.1-android15"
      }  
    },

    "bazel": {
      "target": "//common:kernel_aarch64_dist",
      "configs": [
        "fast"
      ]
    },

    "features": {
      "kernelSU": false,
      "nethunter": false
    },

    "artifacts": {
      "collect": [
        "Image",
        "vmlinux",
        "*.img"
      ]
    }
  }
]
