[
  {
    "name": "pixel8a-akita",
    "androidVersion": "15",
    "kernelVersion": "6.1",
    "buildSystem": "bazel",

    "kernelSources": {
      "common": {
        "repo": "https://android.googlesource.com/kernel/common",
        "branch": "android15-6.1"
      },
      "modules": {
        "repo": "https://android.googlesource.com/kernel/google-modules",
        "branch": "android15-6.1"
      },
      "device": {
        "repo": "https://android.googlesource.com/kernel/google-devices/akita",
        "branch": "android15-6.1"
      }
    },

    "bazel": {
      "target": "//common:kernel_aarch64_dist",
      "configs": [
        "fast"
      ]
    },

    "features": {
      "kernelSU": false,
      "nethunter": false
    },

    "artifacts": {
      "collect": [
        "Image",
        "vmlinux",
        "*.img"
      ]
    }
  }
]
