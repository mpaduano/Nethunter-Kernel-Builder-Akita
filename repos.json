[
  {
    "name": "pixel8a-akita",
    "androidVersion": "15",
    "kernelVersion": "6.1",
    "buildSystem": "bazel",

    "kernelSources": {
      "common": {
        "repo": "https://android.googlesource.com/kernel/common",
        "branch": "android-gs-akita-6.1-android15-qpr2"
      },
      "modules": {
        "repo": "https://android.googlesource.com/kernel/google-modules/soc/gs",
        "branch": "android-gs-akita-6.1-android15-qpr2"
      },
      "device": {
        "repo": "https://android.googlesource.com/kernel/google-devices/akita",
        "branch": "android-gs-akita-6.1-android15-qpr2"
      }
    },

    "bazel": {
      "target": "//common:kernel_aarch64_dist",
      "configs": ["fast"]
    },

    "features": {
      "kernelSU": false,
      "nethunter": false
    },

    "artifacts": {
      "collect": ["Image", "vmlinux", "*.img"]
    }
  }
]
